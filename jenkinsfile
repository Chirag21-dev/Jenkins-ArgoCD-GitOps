pipeline {
	agent any
	tools {
		nodejs 'NodeJS'
	}
	environment {
		DOCKER_HUB_REPO = ''
		DOCKER_HUB_CREDENTIALS_ID = ''
	}
	stages {
		stage('Checkout Github'){
			steps {
			checkout scmGit(branches: [[name: '*/main']], extensions: [], userRemoteConfigs: [[url: 'https://github.com/Chirag21-dev/Jenkins-nodejscicd.git']])
			}
		}
